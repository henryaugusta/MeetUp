<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.senjapagi.meetup">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo_sinarmas"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logo_sinarmas_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">
        <activity android:name=".room_available" />
        <activity android:name=".room_details" />
        <activity android:name=".dashboard_client" />
        <activity android:name=".dashboard_admin" />
        <activity android:name=".admin_website" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>